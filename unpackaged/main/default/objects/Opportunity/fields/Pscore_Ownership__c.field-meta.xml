<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Pscore_Ownership__c</fullName>
    <description>Formula to derive a value based on whether a hospital is privately owned/operated or publicly owned/operated</description>
    <externalId>false</externalId>
    <formula>CASE( Account.Ownership , 
"Public/Government Owned/Operated", 1, 
"Joint Ownership - Private/Public", 1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>*Pscore Ownership</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
